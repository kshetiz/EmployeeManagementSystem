@model IEnumerable<WebApplication1.Data.Entities.EmployeeJobHistory>

@{
    ViewData["Title"] = "GetByID";
}

<h1>GetByID</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Position.PositionName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EmployeeJobHistoryId">Edit</a>
                <a asp-action="Delete" asp-route-id="@item.EmployeeJobHistoryId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
